{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 2,
    "background":{
        "scripts": ["jquery.js","background.js"],
        "persistent":true
    },
    "content_scripts": [
        {
          "matches": ["https://www.codechef.com/*/submit/*"],
          "js": ["contentScript.js"]
        }
      ],
    "permissions":[
        "storage",
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "activeTab",
        "*://*/*"
    ]
  }